{
  "name": "api-gudnus",
  "version": "1.0.0",
  "description": "api for Gudnus Web app",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brianjeball/api-gudnus.git"
  },
  "keywords": [
    "gudnus"
  ],
  "author": "Brian J. Ball",
  "license": "ISC",
  "bugs": {
